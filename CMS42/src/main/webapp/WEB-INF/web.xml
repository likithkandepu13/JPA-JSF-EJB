<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletAdminRegistration</servlet-name>
  	<display-name>ServletAdminRegistration</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletAdminRegistration</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletAdminLogin</servlet-name>
  	<display-name>ServletAdminLogin</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletAdminLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletAddCar</servlet-name>
  	<display-name>ServletAddCar</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletAddCar</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletUpdateCar</servlet-name>
  	<display-name>ServletUpdateCar</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletUpdateCar</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDeleteCar</servlet-name>
  	<display-name>ServletDeleteCar</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletDeleteCar</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletViewCar</servlet-name>
  	<display-name>ServletViewCar</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletViewCar</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletAddSalesperson</servlet-name>
  	<display-name>ServletAddSalesperson</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServletAddSalesperson</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletAdminRegistration</servlet-name>
  	<url-pattern>/ServletAdminRegistration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletAdminLogin</servlet-name>
  	<url-pattern>/ServletAdminLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletAddCar</servlet-name>
  	<url-pattern>/ServletAddCar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletUpdateCar</servlet-name>
  	<url-pattern>/ServletUpdateCar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletDeleteCar</servlet-name>
  	<url-pattern>/ServletDeleteCar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletViewCar</servlet-name>
  	<url-pattern>/ServletViewCar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletAddSalesperson</servlet-name>
  	<url-pattern>/ServletAddSalesperson</url-pattern>
  </servlet-mapping>
  <taglib>
        <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
        <taglib-location>/WEB-INF/lib/jstl-impl.jar</taglib-location>
    </taglib>

    <taglib>
        <taglib-uri>http://java.sun.com/jsp/jstl/functions</taglib-uri>
        <taglib-location>/WEB-INF/lib/jstl-impl.jar</taglib-location>
    </taglib>
</web-app>
